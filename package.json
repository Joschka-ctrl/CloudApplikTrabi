  "description": "repo Cloud",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "install-all": "npm run install-backend && npm run install-backend-echarging && npm run install-backend-parking && npm run install-backend-reporting && npm run install-backend-tenant-creation && npm run install-facility-management && npm run install-tenant-creation-frontend",
