{
    "info": {
      "name": "Parking Service API",
      "description": "Postman collection to test the Parking Service API",
      "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "item": [
      {
        "name": "Create Parking Spot",
        "request": {
          "method": "POST",
          "header": [
            { "key": "Content-Type", "value": "application/json" }
          ],
          "body": {
            "mode": "raw",
            "raw": "{\n  \"id\": 1,\n  \"occupied\": false\n}"
          },
          "url": {
            "raw": "http://localhost:3033/parkingSpots",
            "protocol": "http",
            "host": ["localhost"],
            "port": "3033",
            "path": ["parkingSpots"]
          }
        }
      },
